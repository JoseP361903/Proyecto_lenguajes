   <!-- Profile Modal -->
    <div id="profileModal" class="custom-modal">
        <div class="custom-modal-content2">
            <!-- Close Button -->
            <span class="custom-modal-close">&times;</span>

            <!-- First Row (Image and Name/Role) -->
            <div class="row-profile">
                <div class="col-md-2">
                    <!-- Profile Image (Placeholder or Actual Image) -->
                    <img src="~/images/defaultpfp.jpg" alt="Profile Picture" class="img-fluid">
                </div>
                <div class="col-md-3">
                    <!-- Name and Role -->
                    <h3><span id="studentName">Nombre</span></h3>
                    <p><span id="studentID">#ID</span></p>
                </div>
                <div class="col-md-4">
                    <!-- Empty Column for spacing -->
                </div>
            </div>

            <!-- Second Row (Details) -->
            <div class="row">
                <div class="col-md-8">
                    <!-- Form for Additional Profile Information -->
                    <div class="row">
                        <div class="col-md-6">
                            <p for="pName">Nombre:</p>
                        </div>
                        <div class="col-md-6">
                            <input type="text" id="pName" name="pName" class="form-control" value="Example">
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <p for="pSname">Apellido:</p>
                        </div>
                        <div class="col-md-6">
                            <input type="text" id="pSname" name="pSname" class="form-control" value="Example">
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <p for="pMail">Correo Electrónico:</p>
                        </div>
                        <div class="col-md-6">
                            <input type="email" id="pMail" name="pMail" class="form-control" value="example@gmail.com">
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <p for="pPic">Foto de perfil</p>
                        </div>
                        <div class="col-md-6">
                            <label for="pPic" class="file-label">Explorar archivos</label>
                            <input type="file" id="pPic" name="pPic" class="form-control">
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <p for="pPass">Contraseñas:</p>
                        </div>
                        <div class="col-md-6">
                            <input type="password" id="pPassword" name="pPass" class="form-control" required>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Third Row (Likings + Buttons) -->
            <div class="row align-items-center">
                <div class="col-md-6">
                    <div class="form-group">
                        <p>Intereses</p>
                        <div class="form-check">
                            <input class="form-check-input" type="radio" id="radioDB" name="intereses" value="databases">
                            <label class="form-check-label" for="radioDB">Bases de datos</label>
                        </div>
                        <div class="form-check">
                            <input class="form-check-input" type="radio" id="radioNetwork" name="intereses" value="networks">
                            <label class="form-check-label" for="radioNetwork">Redes</label>
                        </div>
                        <div class="form-check">
                            <input class="form-check-input" type="radio" id="radioCSecurity" name="intereses" value="cybersecurity">
                            <label class="form-check-label" for="radioCSecurity">Ciberseguridad</label>
                        </div>
                        <div class="form-check">
                            <input class="form-check-input" type="radio" id="radioDevelopment" name="intereses" value="development">
                            <label class="form-check-label" for="radioDevelopment">Desarrollo</label>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 text-right"></div>

                <div class="row align-items-center">
                    <div class="col-md-6 text-right">
